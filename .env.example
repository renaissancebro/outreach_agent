# Environment Variables for Outreach Agent

# OpenAI API Key (required for AI email generation)
OPENAI_API_KEY=your_openai_api_key_here

# Snov.io Authentication (choose ONE method)
# Method 1: OAuth Client Credentials (recommended)
SNOV_CLIENT_ID=your_snov_client_id_here
SNOV_CLIENT_SECRET=your_snov_client_secret_here

# Method 2: API Key (alternative)
# SNOV_API_KEY=your_snov_api_key_here

# SerpAPI Key (optional - for search-based lead collection)
SERPAPI_KEY=your_serpapi_key_here

# Optional: Rate limiting and other settings
RATE_LIMIT_DELAY=2
